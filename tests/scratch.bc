core :: module("core");

vector3 :: struct {
    x, y, z: f32;
};

#run {
    a, b := 6, 7;
    b, a := a, b;

    core::print("a, b := %d, %d\n", a, b);

    pos1: vector3;
    with pos1 {
        .x := 1.0;
        .y := 1.0;
        .z := 1.0;
    };
    //pos2 := pos1;

    core::print("pos1.x := %f\n", pos1.x);
    core::print("pos1.y := %f\n", pos1.y);
    core::print("pos1.z := %f\n", pos1.z);
};