core :: module("core");

vector3 :: struct {
    x, y, z: f32 := 1.0;
};

#run {
    core::print("before\n\n");

    one: vector3;
    core::print("one.x := %f\n", one.x);
    core::print("one.y := %f\n", one.y);
    core::print("one.z := %f\n", one.z);

    two: vector3;
    core::print("\ntwo.x := %f\n", two.x);
    core::print("two.y := %f\n", two.y);
    core::print("two.z := %f\n", two.z);

    one.x := -3.0;
    one.y := -3.0;
    one.z := -16.5;

    core::print("after\n\n");
    two := one;
    core::print("two.x := %f\n", two.x);
    core::print("two.y := %f\n", two.y);
    core::print("two.z := %f\n", two.z);
};