core :: module("../modules/core");

#run {
    x := 1;

    switch x {
        case 0 => {
            print("0\n");
        };

        case 1 => {
            print("1\n");
        };

        case 2 => {
            print("2\n");
            fallthrough;
        };

        case => {
            print("default\n");
        };
    };
};