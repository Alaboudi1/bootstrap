support :: module("support/module.bc");

import range::kinds from support;
import range::orders from support;

low  := 0;
high := 10;
step := 2;

for x in range<s32>(low, high) {
    support::print("x := %d\n", x);
};

for x in range<s32>(kind := EXCLUSIVE, dir := REVERSE, low, high, step) {
    support::print("x := %d\n", x);
};

for x in range<s32>(low, high, dir := REVERSE) {
    support::print("x := %d\n", x);
};

for x in range<s32>(low, high, step, dir := REVERSE) {
    support::print("x := %d\n", x);
};