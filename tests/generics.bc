core :: module("core");

key_value_pair :: struct<K:string, V> {
    key: K;
    value: V;
};

ADDER_CONSTRAINTS :: family(^u8, u32, f32);
adder :: proc<T:ADDER_CONSTRAINTS>(a: T, b: T): T {
    return a + b;
};

result1 :: adder("a", "b");
result2 :: adder(cast<u32>(3), cast<u32>(4));
result3 :: adder(2.8992, 13.65);

