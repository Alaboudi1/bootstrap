support :: module("support/module.bc");

i: u32 := 1;
while i <= 20 {
    support::print("i := %02d ... ", i);
    if (i % 15) == 0 {
        support::print("fizzbuzz\n");
    } else if (i % 5) == 0 {
        support::print("buzz\n");
    } else if (i % 3) == 0 {
        support::print("fizz\n");
    } else {
        support::print("zzzzz\n");
    };
    i +:= 1;
};

j: u32 := 0;
k: u32 := 0;
while true {
    k +:= 1;

    support::print("j := %02d\n", j);

    if (k % 4) == 0 {
        continue;
    };

    if j == 6 {
        continue 'double_it:;
    };

    if j == 20 {
        break;
    };

    j +:= 1;
    continue;

'double_it:
    j +:= 4;
};

support::print("after: j := %02d\n", j);
