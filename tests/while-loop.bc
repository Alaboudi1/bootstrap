@library "alpha-core";
#foreign print ::= proc(format: string, values: ...any) @alias "fmt_print";

i: u32 := 1;

while i <= 20 {
    print("i := %02d ... ", i);
    if (i % 15) == 0 {
        print("fizzbuzz\n");
    } else if (i % 5) == 0 {
        print("buzz\n");
    } else if (i % 3) == 0 {
        print("fizz\n");
    } else {
        print("zzzzz\n");
    };
    i := i + 1;
};