@library "alpha-core";
#foreign print ::= proc(format: string, values: ...any) @alias "fmt_print";

i: u32 := 1;
while i <= 20 {
    print("i := %02d ... ", i);
    if (i % 15) == 0 {
        print("fizzbuzz\n");
    } else if (i % 5) == 0 {
        print("buzz\n");
    } else if (i % 3) == 0 {
        print("fizz\n");
    } else {
        print("zzzzz\n");
    };
    i := i + 1;
};

j: u32 := 0;
k: u32 := 0;
while true {
    k := k + 1;

    print("j := %02d\n", j);

    if (k % 4) == 0 {
        continue;
    };

    if j == 6 {
        continue 'double_it:;
    };

    if j == 20 {
        break;
    };

    j := j + 1;
    continue;

'double_it:
    j := j + 4;
};

print("after: j := %02d\n", j);
