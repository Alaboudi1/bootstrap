support ::= module("support/module.bc");

// range/xrange intrinsic, params are start, stop,
//      step (can go from low to high or high to low)
for x in range(0, 10) {
    support::print("x := %d\n", x);
};

// step by 2
for x in range(0, 10, 2) {
    support::print("x := %d\n", x);
};

// reverse
for x in range(10, 0) {
    support::print("x := %d\n", x);
};

// reverse with step
for x in range(10, 0, 2) {
    support::print("x := %d\n", x);
};

// 2. iterating over arrays
for it in array<u8>(1, 2, 3, 4, 5) {
    support::print("x := %d\n", x);
};

// 3. iterating over maps
for it in map<string, string>("foo" := "bar", "bing" := "baz") {
    support::print("k := %s\n", k);
    support::print("v := %d\n", v);
}

